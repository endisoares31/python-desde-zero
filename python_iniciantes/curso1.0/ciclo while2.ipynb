   "source": [
    "# soma de numero sucessivos\n",
    "# som = 1 + 2 + 3 + 4...+1000\n",
    "\n",
    "n = int(input(\"Inserir um numero inteiro: \"))\n",
    "\n",
    "sum = 0\n",
    "\n",
    "i = 1 \n",
    "while i < n: # True\n",
    "    sum = sum + i # = 0 + 1\n",
    "\n",
    "    i = i + 1  \n",
    "\n",
    "\n"
   ]
